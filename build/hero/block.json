{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "custom-plugin/hero",
  "version": "0.1.0",
  "title": "Hero",
  "category": "design",
  "icon": "cover-image",
  "description": "Hero component",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "hero",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "mediaId": {
      "type": "number",
      "default": 0
    },
    "mediaUrl": {
      "type": "string",
      "default": ""
    },
    "text": {
      "type": "string",
      "source": "text",
      "selector": "div",
      "default": ""
    },
    "minHeight": {
      "type": "number",
      "default": 400
    }
  }
}